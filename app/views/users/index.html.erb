<h1>ユーザー一覧</h1>
<div>
  <%= form_tag import_users_path, multipart: true do %>
    <%= file_field_tag :file, accept: 'text/csv' %>
    <%= submit_tag "インポート" %>
  <% end %>
  <%= link_to "CSV出力", users_path(format: :csv) %>

</div>

<table>
  <thead>
  <tr>
    <th>ID</th>
    <th>名前</th>
    <th>年齢</th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.age %></td>
    </tr>
  <% end %>
  </tbody>
</table>
